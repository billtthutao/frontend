 () => {\r\n        let selectNum = parseInt(this.myRef1.current.value);\r\n        this.props.incrementAsync(selectNum*1,1000)\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div>\r\n                <select ref={this.myRef1}>\r\n                    <option value=\"1\">1</option>\r\n                    <option value=\"2\">2</option>\r\n                    <option value=\"3\">3</option>\r\n                    <option value=\"4\">4</option>\r\n                </select>\r\n                <input onClick={this.add} type=\"button\" value=\"+\"></input>&nbsp;\r\n                <input onClick={this.subscribe} type=\"button\" value=\"-\"></input>&nbsp;\r\n                <input onClick={this.addIfOdd} type=\"button\" value=\"Increamt if odd\"></input>&nbsp;\r\n                <input onClick={this.addAsync} type=\"button\" value=\"Increamt async\"></input>&nbsp;\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default connect(\r\n    state=>({count:state.count}),\r\n    {\r\n        increment,\r\n        decrement,\r\n        incrementAsync\r\n    })\r\n    (Panel)","import {ADDPERSON} from '../../redux/count_const'\r\n\r\nexport const addPerson = (data) => ({type:ADDPERSON,data})","import React, { Component } from 'react'\r\nimport {connect} from 'react-redux'\r\nimport {addPerson} from '../../redux/actions/person'\r\nimport {nanoid} from 'nanoid'\r\n\r\nclass Person extends Component {\r\n    personName = React.createRef();\r\n    personAge = React.createRef();\r\n\r\n    addPerson = () => {\r\n        const person = {id: nanoid(), name: this.personName.current.value, age: this.personAge.current.value*1}\r\n        this.props.addPerson(person)\r\n        this.personName.current.value = ''\r\n        this.personAge.current.value = ''\r\n    }\r\n\r\n    render() {\r\n        let {count} = this.props;\r\n\r\n        return (\r\n            <div>\r\n                <h2>我是Person组件,Count组件的结算结果为{count}</h2>\r\n                <hr></hr>\r\n     